// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id      String @unique
  address String
  Cart    Cart[]
}

model Cart {
  id         Int    @unique
  totalPrice String
  user       User   @relation(references: [id], fields: [userId])
  userId     String
}
